# Mediflux LangChain Configuration
# Copy this file to .env and fill in your values

# XAI (Grok) API Configuration - Primary LLM
XAI_API_KEY=your_xai_api_key_here
XAI_MODEL=grok-2
XAI_TEMPERATURE=0.3

# OpenAI API Configuration - Fallback LLM
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-0125-preview
OPENAI_TEMPERATURE=0.3

# Annuaire Sant√© API
ANNUAIRE_SANTE_API_KEY=your_annuaire_sante_api_key_here

# LangSmith Configuration (Optional - for monitoring)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=mediflux-healthcare

# Environment
ENVIRONMENT=development

# Database
DATABASE_URL=sqlite:///./mediflux_v2.db

# Logging
LOG_LEVEL=INFO

# Healthcare Data Sources (Optional - for external APIs)
BDPM_API_URL=https://base-donnees-publique.medicaments.gouv.fr
ANNUAIRE_SANTE_API_URL=https://annuaire.sante.fr
